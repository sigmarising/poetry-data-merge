{
    "dynasty": "宋",
    "poet": "陈绎",
    "poem": [
        {
            "title": "寿昌寺碧溪轩",
            "content": "萧疏华薄隐山村，清浅浮桁带郭门。竹倚栏杆水边寺，兴来题作碧溪轩。",
            "comment": ""
        },
        {
            "title": "句",
            "content": "榕叶堂前生昼阴。",
            "comment": ""
        }
    ]
}