{
    "dynasty": "宋",
    "poet": "左誉",
    "poem": [
        {
            "title": "失调名",
            "content": "帷云剪水，滴粉搓酥。",
            "comment": ""
        },
        {
            "title": "失调名",
            "content": "无所事，盈盈秋水，淡淡春山。",
            "comment": ""
        },
        {
            "title": "失调名",
            "content": "一段离愁堪画处，横风斜雨摇衰柳。",
            "comment": ""
        }
    ]
}